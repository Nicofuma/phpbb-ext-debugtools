{
	"name": "nicofuma/debugtools",
	"type": "phpbb-extension",
	"description": "Additional debug tools for phpBB.",
	"homepage": "https://www.phpbb.com",
	"version": "1.1.0",
	"keywords": ["phpbb", "extension", "debug"],
	"license": "GPL-2.0",
	"authors": [
		{
			"name": "Tristan Darricau",
			"email": "phpbb@nicofuma.fr",
			"role": "Lead Developer"
		}
	],
	"require": {
		"php": ">=5.3.3",
		"composer/installers": "~1.0"
	},
	"require-dev": {
		"phpbb/epv": "dev-master"
	},
	"extra": {
		"display-name": "Debug Tools",
		"soft-require": {
			"phpbb/phpbb": "3.2.*@dev"
		},
		"version-check": {
			"host": "nicofuma.github.io",
			"directory": "/",
			"filename": "debugtools.json"
		}
	}
}
